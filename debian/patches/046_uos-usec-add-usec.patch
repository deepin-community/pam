+
+#include "config.h"
+
+#include <errno.h>
+#include <limits.h>
+#include <pwd.h>
+#include <stdio.h>
+#include <stdlib.h>
+#include <string.h>
+#include <unistd.h>
+#include <sys/types.h>
+#include <sys/stat.h>
+#include <fcntl.h>
+#include <syslog.h>
+
+#include <security/pam_modules.h>
+#include <security/_pam_macros.h>
+#include <security/pam_modutil.h>
+#include <security/pam_ext.h>
+#include "pam_inline.h"
+
+#include <selinux/selinux.h>
+#include <selinux/get_context_list.h>
+#include <selinux/context.h>
+#include <selinux/get_default_type.h>
+
+#ifdef HAVE_LIBAUDIT
+#include <libaudit.h>
+#include <sys/select.h>
+#endif
+
+#define DATANAME "pam_usec_context"
+typedef struct {
+  unsigned int usec_user;
+  unsigned int prev_usec_user;
+  char *tty_path;
+} module_data_t;
+
+static void
+free_module_data(module_data_t *data)
+{
+    free(data->tty_path);
+    free(data);
+}
+
+static void
+cleanup(pam_handle_t *pamh UNUSED, void *data, int err UNUSED)
+{
+  free_module_data(data);
+}
+
+static const module_data_t *
+get_module_data(const pam_handle_t *pamh)
+{
+  const void *data;
+
+  return (pam_get_data(pamh, DATANAME, &data) == PAM_SUCCESS) ? data : NULL;
+}
+
+static const char *
+get_item(const pam_handle_t *pamh, int item_type)
+{
+  const void *item;
+
+  return (pam_get_item(pamh, item_type, &item) == PAM_SUCCESS) ? item : NULL;
+}
+
+typedef struct {
+    char username[32];
+    uint8_t value;
+} ContextEntry;
+
+#define MAX_ENTRIES 8
+int parse_context_file(const char *filename, ContextEntry entries[], int max_entries) {
+    FILE *file = fopen(filename, "r");
+    if (!file) {
+        return -1;
+    }
+
+    char line[128];
+    int count = 0;
+
+    while (fgets(line, sizeof(line), file) && count < max_entries) {
+        line[strcspn(line, "\n")] = '\0';
+        if (line[0] == '\0' || line[0] == '#') {
+            continue;
+        }
+
+        char *name = strtok(line, " \t");
+        char *value_str = strtok(NULL, " \t");
+
+        if (name && value_str) {
+            uint8_t value;
+            if (sscanf(value_str, "0x%hhx", &value) == 1) {
+                strncpy(entries[count].username, name, sizeof(entries[count].username) - 1);
+                entries[count].username[sizeof(entries[count].username) - 1] = '\0';
+                entries[count].value = value;
+                count++;
+            } else {
+                fprintf(stderr, "Invalid value format: %s\n", value_str);
+            }
+        }
+    }
+
+    fclose(file);
+    return count;
+}
+
+static int
+compute_tty_context(const pam_handle_t *pamh, module_data_t *data)
+{
+    char *user_name;
+    int retval;
+    const char *filename = "/etc/usec/default/contexts/default_type";
+    ContextEntry entries[MAX_ENTRIES];
+
+    int num_entries = parse_context_file(filename, entries, MAX_ENTRIES);
+    if (num_entries < 0) {
+        return EXIT_FAILURE;
+    }
+
+    int i, debug = 0, verbose = 0, close_session = 0, restore = 0;
+
+    retval = pam_get_item(pamh, PAM_USER, (void *) &user_name);
+    if (retval != PAM_SUCCESS) {
+        return PAM_AUTH_ERR;
+    }
+
+    data->usec_user = 0xffffffff;
+    for (int i=0; i<MAX_ENTRIES; i++) {
+        if (strcmp(entries[i].username, "default") == 0) {
+            data->usec_user = entries[i].value << 24;
+            continue;
+        }
+
+        if (strcmp(entries[i].username, user_name) == 0) {
+            data->usec_user = entries[i].value << 24;
+            break;
+        }
+    }
+
+    const char *tty = get_item(pamh, PAM_TTY);
+
+    pam_syslog(pamh, LOG_INFO, "tty = %s", tty);
+
+    if (!tty || !*tty || !strcmp(tty, "ssh")
+        || pam_str_skip_prefix(tty, "NODEV") != NULL) {
+        tty = ttyname(STDIN_FILENO);
+        if (!tty || !*tty)
+        tty = ttyname(STDOUT_FILENO);
+        if (!tty || !*tty)
+        tty = ttyname(STDERR_FILENO);
+        if (!tty || !*tty)
+        return PAM_SUCCESS;
+    }
+
+    if (pam_str_skip_prefix(tty, "/dev/") == NULL) {
+        if (asprintf(&data->tty_path, "%s%s", "/dev/", tty) < 0)
+        data->tty_path = NULL;
+    } else {
+        data->tty_path = strdup(tty);
+    }
+
+    if (!data->tty_path) {
+        pam_syslog(pamh, LOG_CRIT, "Out of memory");
+        return PAM_BUF_ERR;
+    }
+
+  return PAM_SUCCESS;
+}
+
+static int
+restore_context(const pam_handle_t *pamh, const module_data_t *data, int debug)
+{
+    syslog(LOG_ERR, "restore_context");
+    int err;
+
+    if (!data) {
+        if (debug)
+        pam_syslog(pamh, LOG_NOTICE, "No context to restore");
+        return PAM_SUCCESS;
+    }
+
+    return PAM_SUCCESS;
+}
+
+int set_usec_user(pam_handle_t *pamh, const char* path, unsigned int usec_user)
+{
+    int fd = open(path, O_WRONLY);
+    if (fd == -1) {
+        pam_syslog(pamh, LOG_CRIT, "Failed to open file for writing %s", path);
+        syslog(LOG_ERR, "Failed to open file for writing %s", path);
+        return -1;
+    }
+
+    ssize_t bytes_written = write(fd, &usec_user, sizeof(usec_user));
+    if (bytes_written == -1) {
+        pam_syslog(pamh, LOG_CRIT, "Error writing to file %s %d", path, errno);
+        syslog(LOG_ERR, "Error writing to file %s %d", path, errno);
+        close(fd);
+        return -1;
+    }
+
+    close(fd);
+    return 0;
+}
+
+static int
+set_context(pam_handle_t *pamh, const module_data_t *data,
+	    int debug, int verbose)
+{
+    pid_t self_pid = getpid();
+    char cmd[256] = {0};
+    
+    int rc, err;
+
+    if (data->tty_path)
+    {
+        lsetxattr(data->tty_path, "security.usec_user", &data->usec_user, sizeof(data->usec_user), 0);
+        snprintf(cmd, sizeof(cmd)-1, "/usr/sbin/setusecuser %d 0x%x %s", self_pid, data->usec_user, data->tty_path);
+    }
+    else {
+        snprintf(cmd, sizeof(cmd)-1, "/usr/sbin/setusecuser %d 0x%x", self_pid, data->usec_user);
+    }
+    set_usec_user(pamh, "/proc/self/attr/usec/ubits", data->usec_user);
+    rc = system(cmd);
+    pam_syslog(pamh, LOG_NOTICE, "set_context %s", cmd);
+    return PAM_SUCCESS;
+}
+
+static int
+create_context(pam_handle_t *pamh, int argc, const char **argv,
+	       int debug, int verbose)
+{
+    syslog(LOG_ERR, "create_context");
+  int i;
+  int ttys = 1;
+  int select_context = 0;
+  int use_current_range = 0;
+  int env_params = 0;
+  module_data_t *data;
+
+  /* Parse arguments. */
+  for (i = 0; i < argc; i++) {
+    if (strcmp(argv[i], "nottys") == 0) {
+      ttys = 0;
+    }
+    if (strcmp(argv[i], "select_context") == 0) {
+      select_context = 1;
+    }
+    if (strcmp(argv[i], "use_current_range") == 0) {
+      use_current_range = 1;
+    }
+    if (strcmp(argv[i], "env_params") == 0) {
+      env_params = 1;
+    }
+  }
+
+  if (!(data = calloc(1, sizeof(*data)))) {
+    pam_syslog(pamh, LOG_CRIT, "Out of memory");
+    return PAM_BUF_ERR;
+  }
+
+  if ((i = compute_tty_context(pamh, data)) != PAM_SUCCESS) {
+    free_module_data(data);
+    return i;
+  }
+
+  pam_syslog(pamh, LOG_INFO, "data->tty_path = %s usec-user =%x", data->tty_path, data->usec_user);
+
+  if ((i = pam_set_data(pamh, DATANAME, data, cleanup)) != PAM_SUCCESS) {
+    pam_syslog(pamh, LOG_ERR, "Error saving context: %m");
+    free_module_data(data);
+    return i;
+  }
+
+  return set_context(pamh, data, debug, verbose);
+}
+
+int
+pam_sm_authenticate(pam_handle_t *pamh UNUSED, int flags UNUSED,
+		    int argc UNUSED, const char **argv UNUSED)
+{
+  /* Fail by default. */
+  return PAM_AUTH_ERR;
+}
+
+int
+pam_sm_setcred(pam_handle_t *pamh UNUSED, int flags UNUSED,
+	       int argc UNUSED, const char **argv UNUSED)
+{
+  return PAM_SUCCESS;
+}
+
+int
+pam_sm_open_session(pam_handle_t *pamh, int flags UNUSED,
+		    int argc, const char **argv)
+{
+    char *user_name;
+    int retval;
+
+  const module_data_t *data;
+  int i, debug = 0, verbose = 0, close_session = 0, restore = 0;
+
+  retval = pam_get_item(pamh, PAM_USER, (void *) &user_name);
+
+  pam_syslog(pamh, LOG_INFO, "pam_sm_open_session user %s(uid=%lu)", user_name, (unsigned long)getuid());
+
+  /* Parse arguments. */
+  for (i = 0; i < argc; i++) {
+    syslog(LOG_ERR, "argv[%d] = %s", i, argv[i]);
+    if (strcmp(argv[i], "debug") == 0) {
+      debug = 1;
+    }
+    if (strcmp(argv[i], "verbose") == 0) {
+      verbose = 1;
+    }
+    if (strcmp(argv[i], "close") == 0) {
+      close_session = 1;
+    }
+    if (strcmp(argv[i], "restore") == 0) {
+      restore = 1;
+    }
+  }
+
+  if (debug)
+    pam_syslog(pamh, LOG_NOTICE, "Open Session");
+
+  /* Is this module supposed to execute close_session only? */
+  if (close_session)
+    return PAM_SUCCESS;
+
+  data = get_module_data(pamh);
+
+  /* Is this module supposed only to restore original context? */
+  if (restore)
+    return restore_context(pamh, data, debug);
+
+  /* If there is a saved context, this module is supposed to set it again. */
+  return data ? set_context(pamh, data, debug, verbose) :
+    create_context(pamh, argc, argv, debug, verbose);
+}
+
+int
+pam_sm_close_session(pam_handle_t *pamh, int flags UNUSED,
+		     int argc, const char **argv)
+{
+  int i, debug = 0, open_session = 0;
+
+  /* Parse arguments. */
+  for (i = 0; i < argc; i++) {
+    if (strcmp(argv[i], "debug") == 0) {
+      debug = 1;
+    }
+    if (strcmp(argv[i], "open") == 0) {
+      open_session = 1;
+    }
+  }
+
+  if (debug)
+    pam_syslog(pamh, LOG_NOTICE, "Close Session");
+
+  /* Is this module supposed to execute open_session only? */
+  if (open_session)
+    return PAM_SUCCESS;
+
+  /* Restore original context. */
+  return restore_context(pamh, get_module_data(pamh), debug);
+}
